@import url("/css/textStyles.css");

body {
border:0px; margin:0px; text-align:center; 
font-family: Helvetica, Arial, sans-serif; 
-webkit-font-smoothing: antialiased;
font-smoothing: antialiased;
overflow-y: scroll;
color: #000;
background-color: #ebecf0;
background-position: center top;
background-image: url(/images/main_bg.png);
background-repeat: repeat-x;
-webkit-text-size-adjust: none;

image-rendering: -moz-crisp-edges;
image-rendering:-o-crisp-edges;
image-rendering: webkit-optimize-contrast;
image-rendering: optimizeQuality;
-ms-interpolation-mode: bicubic;
width:100%; min-width:980px;
}

body.fancyPage { overflow-y: auto; }
body.editing { overflow-y: hidden; overflow: hidden; background:transparent; padding:20px; text-align:left; min-width:600px; }

a {color: #cf041d; text-decoration:none; outline:none;}
a.btn {
float:left; margin-top:2px; 
background: url(/images/bg_button.png) left top no-repeat; padding: 11px; width:229px; height:42px; color:#323a45; font-size:15px; }

a.btn:hover {color:#323a45; background: url(/images/bg_button_o.png) left top no-repeat; }

img {border:0px; outline:none; }
a:hover {color: #cf041d;}
a:focus,a:active,img:focus, img:active {outline:none;}
iframe {border:0px; }
h1, h2, h3 {font-size:18px}
:focus {  -moz-outline-style:none;}

#centered { position:relative;	margin: 0 auto; width:100%; max-width:960px; height:100%; text-align: left; z-Index:3;  }

